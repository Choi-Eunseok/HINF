<Window x:Class="HINF.RemoveStudent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HINF"
        mc:Ignorable="d"
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
        Title="학생 삭제" Height="310" Width="310" ResizeMode="NoResize">
    <Grid>
        <ListBox ItemsSource="{Binding Path=EleList}" HorizontalAlignment="Left" Height="225" Margin="10,10,0,0" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="btnOK" Content="삭제" HorizontalAlignment="Left" Margin="152,240,0,0" VerticalAlignment="Top" Width="132" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="취소" HorizontalAlignment="Left" Margin="10,240,0,0" VerticalAlignment="Top" Width="129" Click="btnCancel_Click"/>

    </Grid>
</Window>
